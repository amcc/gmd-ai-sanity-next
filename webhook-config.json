{
  "webhooks": [
    {
      "name": "FAQ Content Updates",
      "url": "https://ai.gmdlcc.com/api/revalidate",
      "httpMethod": "GET",
      "headers": {
        "Content-Type": "application/json"
      },
      "filter": "_type == \"faq\"",
      "projection": {
        "_type": "_type",
        "_id": "_id",
        "question": "question"
      },
      "bodyTemplate": ""
    },
    {
      "name": "Homepage Content Updates",
      "url": "https://ai.gmdlcc.com/api/revalidate",
      "httpMethod": "GET",
      "headers": {
        "Content-Type": "application/json"
      },
      "filter": "_type == \"homepageSingleton\"",
      "projection": {
        "_type": "_type",
        "_id": "_id"
      },
      "bodyTemplate": ""
    }
  ]
}
